'''
Main application file for the drawing app with forecasting.
'''
import tkinter as tk
from tkinter import ttk, colorchooser, filedialog
from PIL import Image, ImageDraw, ImageGrab
import numpy as np
import tensorflow as tf
import os

class DrawingApp:
    '''
    Main class for the drawing application.
    '''
    DEFAULT_PEN_SIZE = 5.0
    DEFAULT_COLOR = 'black'

    def __init__(self, master):
        '''
        Initializes the drawing application.
        '''
        self.master = master
        master.title("Drawing App with Forecasting")

        self.pen_size = self.DEFAULT_PEN_SIZE
        self.pen_color = self.DEFAULT_COLOR
        self.old_x = None
        self.old_y = None
        self.drawing_tool = "pen"
        self.file_path = None

        self.model = self.load_model() # Load the model during initialization

        # UI elements
        self.create_widgets()

    def create_widgets(self):
        '''
        Creates the UI elements of the application.
        '''
        # Frame for controls
        self.controls_frame = ttk.Frame(self.master)
        self.controls_frame.pack(side=tk.TOP, fill=tk.X)

        # Pen size scale
        self.pen_size_label = ttk.Label(self.controls_frame, text="Pen Size:")
        self.pen_size_label.pack(side=tk.LEFT, padx=5)
        self.pen_size_scale = tk.Scale(self.controls_frame, from_=1, to=20, orient=tk.HORIZONTAL, command=self.change_pen_size)
        self.pen_size_scale.set(self.DEFAULT_PEN_SIZE)
        self.pen_size_scale.pack(side=tk.LEFT, padx=5)

        # Color button
        self.color_button = ttk.Button(self.controls_frame, text="Color", command=self.choose_color)
        self.color_button.pack(side=tk.LEFT, padx=5)

        # Reset button
        self.reset_button = ttk.Button(self.controls_frame, text="Reset", command=self.reset)
        self.reset_button.pack(side=tk.LEFT, padx=5)

        # Predict button
        self.predict_button = ttk.Button(self.controls_frame, text="Predict", command=self.predict_drawing)
        self.predict_button.pack(side=tk.LEFT, padx=5)

        # Clear prediction button
        self.clear_prediction_button = ttk.Button(self.controls_frame, text="Clear Prediction", command=self.clear_prediction)
        self.clear_prediction_button.pack(side=tk.LEFT, padx=5)

        # Save Button
        self.save_button = ttk.Button(self.controls_frame, text="Save", command=self.save)
        self.save_button.pack(side=tk.LEFT, padx=5)

        # Load Button
        self.load_button = ttk.Button(self.controls_frame, text="Load", command=self.load)
        self.load_button.pack(side=tk.LEFT, padx=5)

        # Canvas
        self.canvas = tk.Canvas(self.master, bg='white', width=600, height=400)
        self.canvas.pack(expand=tk.YES, fill=tk.BOTH)
        self.canvas.bind('<B1-Motion>', self.paint)
        self.canvas.bind('<ButtonRelease-1>', self.reset)

        # Prediction Label
        self.prediction_label = ttk.Label(self.master, text="Prediction: ")
        self.prediction_label.pack(pady=5)

    def save(self):
        '''
        Saves the current drawing to a file.
        '''
        try:
            self.file_path = filedialog.asksaveasfilename(defaultextension=".png")
            if self.file_path:
                x = self.master.winfo_rootx() + self.canvas.winfo_x()
                y = self.master.winfo_rooty() + self.canvas.winfo_y()
                x1 = x + self.canvas.winfo_width()
                y1 = y + self.canvas.winfo_height()
                ImageGrab.grab().crop((x, y, x1, y1)).save(self.file_path)
        except Exception as e:
            print(f"Error saving image: {e}")

    def load(self):
        '''
        Loads a drawing from a file.
        '''
        try:
            self.file_path = filedialog.askopenfilename(defaultextension=".png")
            if self.file_path:
                img = Image.open(self.file_path)
                self.canvas.delete("all")
                self.canvas.image = tk.PhotoImage(file=self.file_path)  # Keep a reference!
                self.canvas.create_image(0, 0, image=self.canvas.image, anchor='nw')
        except Exception as e:
            print(f"Error loading image: {e}")

    def change_pen_size(self, new_size):
        '''
        Changes the pen size.
        '''
        self.pen_size = float(new_size)

    def choose_color(self):
        '''
        Opens a color chooser dialog and sets the pen color.
        '''
        self.pen_color = colorchooser.askcolor(color=self.pen_color)[1]

    def paint(self, event):
        '''
        Draws on the canvas.
        '''
        if self.old_x and self.old_y:
            self.canvas.create_line(self.old_x, self.old_y, event.x, event.y,
                                    width=self.pen_size, fill=self.pen_color,
                                    capstyle=tk.ROUND, smooth=tk.TRUE, splinesteps=36)
        self.old_x = event.x
        self.old_y = event.y

    def reset(self):
        '''
        Resets the drawing state.
        '''
        self.old_x, self.old_y = None, None
        self.clear_prediction()

    def clear_canvas(self):
        '''
        Clears the canvas.
        '''
        self.canvas.delete("all")

    def predict_drawing(self):
        '''
        Predicts the drawing using the loaded model.
        '''
        # Get the bounding box of the canvas
        x = self.master.winfo_rootx() + self.canvas.winfo_x()
        y = self.master.winfo_rooty() + self.canvas.winfo_y()
        x1 = x + self.canvas.winfo_width()
        y1 = y + self.canvas.winfo_height()

        # Grab the canvas content as an image
        img = ImageGrab.grab().crop((x, y, x1, y1)).resize((28, 28)).convert('L')
        img_array = np.array(img)
        img_array = img_array.reshape(1, 28, 28, 1) / 255.0

        # Make prediction
        prediction = self.model.predict(img_array)
        predicted_digit = np.argmax(prediction)

        # Update the prediction label
        self.prediction_label.config(text=f"Prediction: {predicted_digit}")

    def clear_prediction(self):
        '''
        Clears the prediction label.
        '''
        self.prediction_label.config(text="Prediction: ")

    def load_model(self):
        '''
        Loads the pre-trained MNIST model.
        '''
        try:
            # Load the model
            model = tf.keras.models.load_model("mnist_model.h5")
            print("Model loaded successfully.")
            return model
        except Exception as e:
            print(f"Error loading model: {e}")
            return None

if __name__ == '__main__':
    root = tk.Tk()
    app = DrawingApp(root)
    root.mainloop()
